    },
    "require-dev": {
        "eventviva/php-image-resize": ">=1.4",
        "squizlabs/php_codesniffer": ">=2",
        "phpmd/phpmd": ">=2.2.2",
        "nubera-ebusiness/intercom-php": "dev-master",
        "phpunit/phpunit": ">=4.6.6",
        "phpunit/phpunit-skeleton-generator": ">=2.0.1",
